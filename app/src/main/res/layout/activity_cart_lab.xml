<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".CartLabActivity">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="103dp"
        android:layout_height="26dp"
        android:layout_marginTop="112dp"
        android:text="Select Date"
        android:textColor="@color/yellowLight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listViewCL"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/buttonCartDatePicker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="28/02/2024"
        app:layout_constraintBottom_toTopOf="@+id/buttonCLBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.306"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/buttonCartTimePicker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10:00"
        app:layout_constraintBottom_toTopOf="@+id/buttonCLAddToCart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.903"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.327"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="85dp"
        android:layout_height="20dp"
        android:layout_marginStart="292dp"
        android:layout_marginTop="104dp"
        android:text="Select Time"
        android:textColor="@color/yellowLight"
        app:layout_constraintBottom_toTopOf="@+id/buttonCLBack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listViewCL"
        app:layout_constraintVertical_bias="0.096"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@id/buttonCLBack"
        android:layout_width="128dp"
        android:layout_height="45dp"
        android:layout_marginBottom="24dp"
        android:background="@color/purpleDark"
        android:text="BACK"
        android:textColor="@color/white"
        android:textColorHighlight="#230168"
        android:textColorLink="#290133"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textViewCLPackageDetails"
        android:layout_width="261dp"
        android:layout_height="47dp"
        android:text="Lab Test Cart Items"
        android:textAlignment="center"
        android:textColor="@color/yellowLight"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.162"
        tools:ignore="DuplicateIds,HardcodedText,TextSizeCheck" />

    <TextView
        android:id="@+id/textViewCartName"
        android:layout_width="301dp"
        android:layout_height="54dp"
        android:text="@string/_24_7_healthcare"
        android:textColor="@color/purple_200"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.576"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textViewCartTitle"
        android:layout_width="246dp"
        android:layout_height="33dp"
        android:text="Deliver location : PUNE"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115"
        tools:ignore="DuplicateIds,HardcodedText,TextSizeCheck" />

    <Button
        android:id="@id/buttonCLAddToCart"
        android:layout_width="131dp"
        android:layout_height="47dp"
        android:layout_marginBottom="24dp"
        android:background="@color/purple_700"
        android:text="CHECKOUT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textViewCLTotalCost"
        android:layout_width="199dp"
        android:layout_height="34dp"
        android:layout_marginTop="36dp"
        android:text="Total Cost : 1298/-"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listViewCL"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ListView
        android:id="@id/listViewCL"
        android:layout_width="360dp"
        android:layout_height="318dp"
        android:layout_margin="10dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.43" />


</androidx.constraintlayout.widget.ConstraintLayout>